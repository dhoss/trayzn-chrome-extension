{
  "name": "Trayzn bookmark extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "webRequest",
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:8080/*"
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}