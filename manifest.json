{
  "name": "Trayzn bookmark extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "webRequest",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "http://localwsl.com:8080/api/v1/bookmarks/add"
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png"
  },
  "background": {
    "service_worker": "trayzn.js"
  },
  "action": {}
}